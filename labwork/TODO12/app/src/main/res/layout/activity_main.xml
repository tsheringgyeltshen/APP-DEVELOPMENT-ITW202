<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/droiddessert"
        android:layout_width="269dp"
        android:layout_height="70dp"
        android:gravity="center"
        android:text="Droid Desserts"
        android:textAlignment="center"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.034" />

    <ImageView
        android:id="@+id/donut"
        android:layout_width="110dp"
        android:layout_height="120dp"
        android:onClick="donutclick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.141"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.234"
        app:srcCompat="@drawable/donut" />

    <ImageView
        android:id="@+id/cupcake"
        android:layout_width="110dp"
        android:layout_height="117dp"
        android:onClick="cupcakeclick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.141"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.57"
        app:srcCompat="@drawable/cupcake" />

    <ImageView
        android:id="@+id/gingerbread"
        android:layout_width="109dp"
        android:layout_height="122dp"
        android:onClick="gingerbreadclick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.877"
        app:srcCompat="@drawable/gingerbread" />

    <TextView
        android:id="@+id/donuttext"
        android:layout_width="204dp"
        android:layout_height="121dp"
        android:gravity="center"
        android:text="ག་ནི་བ་་ཞིམ་པའི་ donut ་འདི་་མོང་སྒར ཚེ་་རིང་ཚོང་ཁང་ལས་ འཐོབ་ཚུ༑"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.233"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/gingerbreadtext"
        android:layout_width="201dp"
        android:layout_height="114dp"
        android:gravity="center"
        android:text="ག་ནི་བ་་ཞིམ་པའི gingerbread ་འདི་་མོང་སྒར ཚེ་་རིང་ཚོང་ཁང་ལས་ འཐོབ་ཚུ༑"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.877"
        tools:ignore="RtlCompat" />

    <TextView
        android:id="@+id/cupcaketext"
        android:layout_width="201dp"
        android:layout_height="112dp"
        android:gravity="center"
        android:text="ག་ནི་བ་་ཞིམ་པའི་ cupcake འདི་་མོང་སྒར ཚེ་་རིང་ཚོང་ཁང་ལས་ འཐོབ་ཚུ༑"
        android:textAlignment="viewStart"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.567"
        tools:ignore="RtlCompat" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/callcall"
        android:layout_width="102dp"
        android:layout_height="59dp"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/ic_menu_call" />


</androidx.constraintlayout.widget.ConstraintLayout>